<html>
    <head>
        <title>{{ title }}</title>
    </head>
    <body>
        <h1>{{ title }}</h1>
        <ul>
            {% for order in orders %}
            <li><b>{{ order.customer_name }}</b>:
            {{ order.order_date }}, 
            {{ order.quantity }}, 
            {{ order.brand }}, 
            {{ order.model }}
            </li>
            {% endfor %}
        </ul>
    </body>
</html>